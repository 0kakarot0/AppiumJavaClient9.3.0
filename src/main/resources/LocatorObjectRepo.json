        "xpath": "//XCUIElementTypeSecureTextField[contains(@name,'Password Text Field')]"
      },
      "loginButton": {
        "accessibilityId": "login_button",
        "xpath": "//XCUIElementTypeStaticText[@name=\"Log in Button\nLog in\"]"
      },
      "errorNotification": {
        "accessibilityId": "login_button",
        "xpath": "//XCUIElementTypeStaticText[contains(@name,'Exception: invalid-credential')]"
      }
